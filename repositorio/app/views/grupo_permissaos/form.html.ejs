<div class="form-panel">	<div class="control-group">		<label for="perfilId" class="control-label">Perfil</label>			<div class="controls">				<%- selectTag(perfils, grupoPermissao.perfilId, {					  name: 'perfilId'					, valueField: 'id'					, textField: 'nome'					, class:'span6 form-control'				}); %>			</div>	</div>	<% if (permissaos) { %>		<div class="control-group">			<label for="permissao" class="form-control">Permissao</label>			<% for (var i = 0, ii = entidades.length; i < ii; i++) { %>				<div class="controls">					<label for="nome" class="form-control"><%= entidades[i].nome %></label>							<% for (var u = 0, uu = permissaos.length; u < uu; u++) { %>					<% if(permissaos[u].entidadeId == entidades[i].id) { %>						<div class="controls">							<input type="checkbox" name="permissaos.nome" class="form-control" value= "<%= permissaos[u].id %>">														<label for="permissaosNome" class="control-label"><%= permissaos[u].nome %></label>						</div>					<% } %>					<% } %>						</div>							<% } %>			</div>			<% } %></div>
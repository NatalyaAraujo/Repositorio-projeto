<div class="hero-unit">
  <h2 class="nome"><%= usuario.nome %></h2>
  <%- linkTo('Edit this usuario', editUsuarioPath(params.id), {class: 'btn pull-right'}); %>
  <%- linkTo('Back to list', usuarioPath(''), {class: 'btn pull-right'}); %>
</div>

<h2>Usuario Properties</h2>
<% for(var i in usuario) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= usuario[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>
<div class="hero-unit">
  <form id="turma-form" class="form-horizontal" action="/turmas" method="POST">
    <fieldset>
      <legend>Create a new Turma</legend>
      <% if(params.errors) { %>
      <div class="control-group">
        <ul>
        <% for(var err in params.errors) { %>
          <li><%= params.errors[err]; %></li>
        <% } %>
        </ul>
      </div>
      <% } %>

      <%- partial('form', {turma: params, usuarios: usuarios}) %>

      <div class="form-actions">
        <%- contentTag('input', 'Add', {type: 'submit', class: 'btn btn-primary', id: 'add-turma'}) %>
      </div>
	  
	  <script>
		$('#add-turma').click(function(){
			console.log("ja disparei");
			$('#add-turma').trigger('Turma-Evento');
		});
	  </script>
    </fieldset>
  </form>
</div>
